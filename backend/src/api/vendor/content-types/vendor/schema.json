{
  "kind": "collectionType",
  "collectionName": "vendors",
  "info": {
    "singularName": "vendor",
    "pluralName": "vendors",
    "displayName": "Vendor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "storeName": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "businessAddress": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "zipcode": {
      "type": "biginteger",
      "required": true
    },
    "fullName": {
      "type": "string",
      "required": true
    },
    "phoneNumber": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "rating": {
      "type": "decimal",
      "required": false,
      "default": 0
    },
    "username": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "bio": {
      "type": "text",
      "maxLength": 200
    },
    "vendorDeliveryFee": {
      "type": "decimal",
      "default": 0
    },
    "paypalEmail": {
      "type": "email"
    }
  }
}
